<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.*?>
<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.TesteController">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="400.0" prefWidth="480.0" VBox.vgrow="ALWAYS">
            <children>
                <Label alignment="CENTER" layoutX="14.0" layoutY="14.0" style="&#10;" text="Cadastro de Pessoa Física"
                       textAlignment="CENTER" textFill="#9f9f9f" wrapText="false">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <Label alignment="CENTER" layoutX="41.0" layoutY="69.0" style="&#10;" text="Nome" textAlignment="CENTER"
                       textFill="#9f9f9f" wrapText="false">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <TextField fx:id="nome" layoutX="41.0" layoutY="95.0"/>
                <Label alignment="CENTER" layoutX="43.0" layoutY="131.0" style="&#10;" text="CPF" textAlignment="CENTER"
                       textFill="#9f9f9f" wrapText="false">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <TextField fx:id="CPF" layoutX="41.0" layoutY="158.0"/>
                <Label alignment="CENTER" layoutX="43.0" layoutY="204.0" style="&#10;" text="Idade"
                       textAlignment="CENTER" textFill="#9f9f9f" wrapText="false">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <TextField fx:id="idade" layoutX="41.0" layoutY="235.0"/>
                <Label alignment="CENTER" layoutX="453.0" layoutY="210.0" style="&#10;" text="Dependentes"
                       textAlignment="CENTER" textFill="#9f9f9f" wrapText="false">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <TextField fx:id="numeroDependentes" layoutX="449.0" layoutY="235.0"/>
                <Label alignment="CENTER" layoutX="452.0" layoutY="145.0" style="&#10;" text="Rendimento Total"
                       textAlignment="CENTER" textFill="#9f9f9f" wrapText="false">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <TextField fx:id="rendimentoTotal" layoutX="452.0" layoutY="172.0"/>
                <Label alignment="CENTER" layoutX="453.0" layoutY="75.0" style="&#10;" text="Contribuição Total"
                       textAlignment="CENTER" textFill="#9f9f9f" wrapText="false">
                    <font>
                        <Font size="18.0"/>
                    </font>
                </Label>
                <TextField fx:id="contribuicaoOficial" layoutX="452.0" layoutY="102.0"/>
                <Button fx:id="btnPagar" layoutX="203.0" layoutY="332.0" mnemonicParsing="false"
                        onAction="#showValorAPagar" prefHeight="41.0" prefWidth="234.0"
                        text="Calcular imposto para pagar"/>
            </children>
        </AnchorPane>
    </children>
</VBox>
